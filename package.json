{
  "name": "libs",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "cz": "cz",
    "fmt": "pnpm lint && pnpm prettier --loglevel=warn --write .",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "bump": "nx run-many --target=version --all",
    "build": "nx run-many --target=build --all",
    "release": "nx run-many --target=publish --all",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@jscutlery/semver": "^2.6.0",
    "@nrwl/cli": "12.8.0",
    "@nrwl/eslint-plugin-nx": "12.8.0",
    "@nrwl/jest": "12.8.0",
    "@nrwl/linter": "12.8.0",
    "@nrwl/node": "^12.8.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.8.0",
    "@nrwl/workspace": "12.8.0",
    "@types/jest": "26.0.24",
    "@types/node": "16.11.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^6.0.0",
    "jest": "27.0.3",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "typescript": "~4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
